<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<navigation-rule>
		<from-view-id>/jakatingkir/login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{login.logAdmin}</from-action>
			<from-outcome>Success</from-outcome>
			<to-view-id>/jakatingkir/index.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/cs/login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{login.logCS}</from-action>
			<from-outcome>Success</from-outcome>
			<to-view-id>/cs/index.jsf</to-view-id>
		</navigation-case>
		
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/investor/login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{login.logInvestor}</from-action>
			<from-outcome>Success</from-outcome>
			<to-view-id>/investor/index.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
